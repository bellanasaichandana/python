test_list = [1, 4, 5, 5, 5, 9, 1]
print("The original list is : " + str(test_list)
oc_set = set()
res = []
for idx, val in enumerate(test_list):
    if val not in oc_set:
        oc_set.add(val)         
    else:
        res.append(idx)     
print("The list of duplicate elements is :  " + str(res))
